@model IEnumerable<NHAMIS.APP.Models.NominationAttachment>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AttachmentType.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nomination.IdNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AttachmentUrl)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AttachmentType.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nomination.IdNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AttachmentUrl)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
